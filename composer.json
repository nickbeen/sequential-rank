{
    "name": "nickbeen/sequential-rank",
    "description": "Simple ranking system for ordering results by sequence",
    "type": "library",
    "keywords": [
        "php",
        "ranking system",
        "sequential rank"
    ],
    "homepage": "https://github.com/nickbeen/sequential-rank",
    "license": "MIT",
    "authors": [
        {
            "name": "Nick Been",
            "email": "nick@nickbeen.nl",
            "homepage": "https://nickbeen.nl",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "NickBeen\\SequentialRank\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NickBeen\\SequentialRank\\Tests\\": "tests"
        }
    },
    "scripts": {
        "tests": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
